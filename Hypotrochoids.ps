/hypotrochoid
{
	% [ (R - r) * cos(t) ] + [ d * cos( ((R-r)/r)*t ) ] == x
	% [ (R - r) * sin(t) ] + [ d * sin( ((R-r)/r)*t ) ] == y
	/bigr exch def
	/littler exch def
	/d exch def
	/t exch def
	bigr littler exch sub t cos mul % first square brackets
	d bigr littler exch sub littler exch div t mul cos mul % second square brackets
	add % x

	bigr littler exch sub t sin mul % first square brackets
	d bigr littler exch sub littler exch div t mul sin mul % second square brackets
	add % y
} def

/inch
{ 72 mul } def

4.25 inch 5.5 inch translate  % moves the center of the image
0 30 360
{
	1 1 720
	{
		newpath
		0 0 moveto
		dup 
		dup 720 div exch 270 div exch 0
		setrgbcolor

		%% t d littler bigr
		110 165 275
		hypotrochoid lineto 
		stroke
	} for
	rotate
} for

showpage
